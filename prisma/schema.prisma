generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  name      String
  email     String   @unique(map: "User_email_key")
  createdAt DateTime @default(now())
  updatedAt DateTime
  password  String
  image     String?

  Log Log[]
}

model Log {
  id          String   @id @unique
  description String
  category    Int?
  createdAt   DateTime @default(now())

  userId      String?
  user        User?    @relation(fields: [userId], references: [id])
}