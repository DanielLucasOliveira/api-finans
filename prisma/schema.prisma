generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(uuid())
  name      String
  email     String    @unique(map: "User_email_key")
  createdAt DateTime? @default(now())
  updatedAt DateTime?
  password  String
  image     String?
}

model Profile {
  id   Int    @id
  type String @unique
}

model Log {
  id          String   @id @default(uuid())
  description String
  category    Int?
  createdAt   DateTime @default(now())
  userId      String?
}
